{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@context": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "instanceTypeId": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "identifiers": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "identifierTypeId": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        },
        "required": [
          "identifierTypeId",
          "value"
        ]
      }
    },
    "creators": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "creatorTypeId": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "creatorTypeId"
        ]
      }
    }
  },
  "required": [
    "title",
    "source",
    "instanceTypeId",
    "creators"
  ]
}
